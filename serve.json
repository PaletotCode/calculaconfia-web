{
  "headers": [
    {
      "source": "index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    },
    {
      "source": "assets/app.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=60, must-revalidate" }
      ]
    },
    {
      "source": "assets/vendor/*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    }
  ]
}

